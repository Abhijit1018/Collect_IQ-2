VIEW CollectIQService_Payers AS SELECT
  Payers_0.createdAt,
  Payers_0.createdBy,
  Payers_0.modifiedAt,
  Payers_0.modifiedBy,
  Payers_0.PayerId,
  Payers_0.PayerName,
  Payers_0.TotalPastDue,
  Payers_0.MaxDaysPastDue,
  Payers_0.Stage,
  Payers_0.ContactEmail,
  Payers_0.ContactPhone,
  Payers_0.Currency,
  Payers_0.LastOutreachStatus,
  Payers_0.lastOutreachAt,
  Payers_0.latestOutreachDraft,
  Payers_0.criticality
FROM my_collectiq_Payers AS Payers_0