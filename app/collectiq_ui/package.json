{
  "name": "collectiq-ui",
  "version": "1.0.0",
  "description": "CollectIQ UI5 Application",
  "scripts": {
    "start": "ui5 serve --open index.html",
    "build": "ui5 build --clean-dest --dest dist",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo --dest dist",
    "lint": "eslint .",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^3.0.0"
  }
}
