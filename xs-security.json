{
  "xsappname": "collectiq",
  "tenant-mode": "dedicated",
  "description": "Security profile for CollectIQ Application",
  "scopes": [
    {
      "name": "$XSAPPNAME.admin",
      "description": "Administrator scope"
    },
    {
      "name": "$XSAPPNAME.user",
      "description": "User scope"
    }
  ],
  "role-templates": [
    {
      "name": "CollectIQAdmin",
      "description": "CollectIQ Administrator",
      "scope-references": [
        "$XSAPPNAME.admin",
        "$XSAPPNAME.user"
      ]
    },
    {
      "name": "CollectIQUser",
      "description": "CollectIQ User",
      "scope-references": [
        "$XSAPPNAME.user"
      ]
    }
  ],
  "attributes": [],
  "oauth2-configuration": {
    "redirect-uris": [
      "http://localhost:4004/**",
      "https://*.trial.applicationstudio.cloud.sap/**",
      "https://*.cfapps.us10-001.hana.ondemand.com/**",
      "https://*.cf.us10-001.hana.ondemand.com/**"
    ]
  }
}
